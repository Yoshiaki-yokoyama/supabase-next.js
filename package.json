{
  "name": "supabase-monorepo",
  "private": true,
  "description": "Supabase Monorepo",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=10.15.1",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "yarn run build:docs && yarn run build:web",
    "build:docs": "yarn workspace @supabase/docs build",
    "build:web": "yarn workspace @supabase/web build",
    "dev": "concurrently --kill-others-on-fail \"yarn dev:web\" \"yarn dev:docs\" ",
    "dev:docs": "yarn workspace @supabase/docs dev",
    "dev:web": "yarn workspace @supabase/web dev",
    "static:web": "yarn workspace @supabase/web static"
  },
  "dependencies": {
    "concurrently": "^5.0.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
